    justify-content: space-between;
    background-color: #f4f4f4;
    width: 80%;
    border-radius: 20px;
    margin-top: 10px;
}

.th, .td {
    display: flex;
    padding: 10px;
    justify-content: space-between;
    width: 100%;
}

.viewBtn {
